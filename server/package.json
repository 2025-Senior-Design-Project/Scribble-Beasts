{
  "name": "scribble-beasts-server",
  "version": "1.0.0",
  "description": "Server for Scribble Beasts game",
  "type": "module",
  "main": "dist/server/src/index.js",
  "exports": "./dist/*.js",
  "scripts": {
    "start": "node dist/server/src/index.js",
    "dev": "npm run generate-assets && tsx watch src/index.ts",
    "generate-assets": "tsx src/lib/scripts/generate-assets-data.ts",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ts-mixer": "^6.0.4",
    "tsx": "^4.20.5",
    "uuid": "^13.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.4",
    "typescript": "^5.0.4"
  }
}
